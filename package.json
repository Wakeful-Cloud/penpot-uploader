{
  "name": "@wakeful-cloud/penpot-uploader",
  "version": "1.0.0",
  "description": "A utility to help bulk-upload SVG files to Penpot as components",
  "author": "Wakeful Cloud",
  "license": "MIT",
  "keywords": [
    "penpot",
    "svg"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.16"
  },
  "bin": {
    "penpot-uploader": "dist/cli.js"
  },
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "ts-node --esm src/cli.ts",
    "lint": "eslint .",
    "start": "node dist/cli.js"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "commander": "^9.3.0",
    "globby": "^13.1.2",
    "jszip": "^3.10.0",
    "svgo": "^2.8.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jszip": "^3.4.1",
    "@types/svgo": "^2.6.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "ts-node": "^10.8.1",
    "tsup": "^6.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wakeful-Cloud/penpot-uploader.git"
  },
  "bugs": {
    "url": "https://github.com/Wakeful-Cloud/penpot-uploader/issues"
  }
}
